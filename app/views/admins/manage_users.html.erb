<div class="span8">
	<div class="well sidebar-nav" >
		<h3>Manage Users</h3>
		<table class="table table-condensed">
			<thead>
			    <tr>
			      <th>Firstname</th>
			      <th>Lastname</th>
			      <th>Email</th>
			      <th>Age</th>
			      <th>Gender</th>
			      <th>Status</th>
			      <th>Register date</th>
			      <th>Edit</th>
			    </tr>
			  </thead>
			  <tbody>
			  	<% @users.each do |user|%>
			    <tr>
			      <td><%= user.firstname %></td>
			      <td><%= user.lastname %></td>
			      <td><%= user.email %></td>
			      <td><%= user.lastname %></td>
			      <td><%= user.gender %></td>
			      <td><%= user.role.id %></td>
			      <td><%= user.created_at.strftime("%d/%m/%Y") %></td>
			      <td>dasds</td>
			    </tr>
			   	<% end %>
	  		   </tbody>
		</table>
	</div>
</div>